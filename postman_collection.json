{
  "info": {
    "name": "AMS API - Appointment Management System",
    "description": "Complete API collection for the SaaS Multi-Tenant Appointment Management System",
    "version": "1.0.0"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{jwt_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "jwt_token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register Tenant",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"Jane\",\n  \"lastName\": \"Smith\",\n  \"email\": \"jane@beautysalon.com\",\n  \"password\": \"password123\",\n  \"role\": \"tenant\",\n  \"tenantData\": {\n    \"name\": \"Beauty Salon\",\n    \"subdomain\": \"beauty-salon\",\n    \"business\": {\n      \"type\": \"salon\",\n      \"description\": \"Full service beauty salon\"\n    }\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/register",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "register"]
            }
          }
        },
        {
          "name": "Register Customer",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\",\n  \"role\": \"customer\",\n  \"tenantId\": \"TENANT_ID_HERE\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/register",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"jane@beautysalon.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/login",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "login"]
            }
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/auth/me",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "me"]
            }
          }
        }
      ]
    },
    {
      "name": "Services",
      "item": [
        {
          "name": "Create Service",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Haircut\",\n  \"description\": \"Professional haircut service\",\n  \"category\": \"beauty\",\n  \"duration\": 60,\n  \"pricing\": {\n    \"basePrice\": 50.00,\n    \"currency\": \"USD\"\n  },\n  \"qualityVariations\": [\n    {\n      \"name\": \"Premium\",\n      \"description\": \"Premium styling products\",\n      \"priceModifier\": {\n        \"type\": \"percentage\",\n        \"value\": 25\n      }\n    }\n  ]\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/services",
              "host": ["{{base_url}}"],
              "path": ["api", "services"]
            }
          }
        },
        {
          "name": "Get Services",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/services?category=beauty",
              "host": ["{{base_url}}"],
              "path": ["api", "services"],
              "query": [
                {
                  "key": "category",
                  "value": "beauty"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Bookings",
      "item": [
        {
          "name": "Create Booking",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"service\": \"SERVICE_ID_HERE\",\n  \"provider\": \"PROVIDER_ID_HERE\",\n  \"customer\": \"CUSTOMER_ID_HERE\",\n  \"appointmentDate\": \"2024-03-15\",\n  \"startTime\": \"10:00\",\n  \"customerNotes\": \"First time customer\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/bookings",
              "host": ["{{base_url}}"],
              "path": ["api", "bookings"]
            }
          }
        },
        {
          "name": "Get Bookings",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/bookings",
              "host": ["{{base_url}}"],
              "path": ["api", "bookings"]
            }
          }
        },
        {
          "name": "Get Provider Calendar",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/bookings/calendar/PROVIDER_ID?startDate=2024-03-01&endDate=2024-03-31",
              "host": ["{{base_url}}"],
              "path": ["api", "bookings", "calendar", "PROVIDER_ID"],
              "query": [
                {
                  "key": "startDate",
                  "value": "2024-03-01"
                },
                {
                  "key": "endDate",
                  "value": "2024-03-31"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Search",
      "item": [
        {
          "name": "Search Services",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/search/services?query=haircut&category=beauty&minPrice=20&maxPrice=100",
              "host": ["{{base_url}}"],
              "path": ["api", "search", "services"],
              "query": [
                {
                  "key": "query",
                  "value": "haircut"
                },
                {
                  "key": "category",
                  "value": "beauty"
                },
                {
                  "key": "minPrice",
                  "value": "20"
                },
                {
                  "key": "maxPrice",
                  "value": "100"
                }
              ]
            }
          }
        },
        {
          "name": "Search Providers by Location",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/search/providers/nearby?latitude=40.7128&longitude=-74.0060&radius=10",
              "host": ["{{base_url}}"],
              "path": ["api", "search", "providers", "nearby"],
              "query": [
                {
                  "key": "latitude",
                  "value": "40.7128"
                },
                {
                  "key": "longitude",
                  "value": "-74.0060"
                },
                {
                  "key": "radius",
                  "value": "10"
                }
              ]
            }
          }
        },
        {
          "name": "Search Tenants",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/search/tenants?query=salon&businessType=beauty",
              "host": ["{{base_url}}"],
              "path": ["api", "search", "tenants"],
              "query": [
                {
                  "key": "query",
                  "value": "salon"
                },
                {
                  "key": "businessType",
                  "value": "beauty"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        }
      }
    }
  ]
}